{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Backend",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/server",
      "cwd": "${workspaceFolder}/cmd/server",
      "postDebugTask": "Delete Running Containers"
    },
    {
      "name": "Frontend",
      "type": "chrome",
      "request": "launch",
      "cwd": "${workspaceFolder}/frontend",
      "url": "http://localhost:3000"
    }
  ]
}
